{
  "components/f/products/stock": "<div class=\"form-field form-field--stock\">\n  <label class=\"form-label form-label--alternate {{#unless product.out_of_stock}}form-label--stock{{else}}form-label--out-of-stock{{/unless}}\">\n    {{#if product.out_of_stock}}\n      <div class=\"productView__stock card-stock--outStock\">{{lang 'products.cards.out_of_stock'}}</div>\n    {{else}}\n      {{#if product.stock_level '<=' product.low_stock_level}}\n        <div class=\"productView__stock card-stock--lowStock\">{{lang 'products.cards.low_stock' quantity=product.stock_level}}</div>\n      {{else}}\n        <div class=\"productView__stock card-stock--inStock\">{{lang 'products.cards.in_stock' quantity=product.stock_level}}</div>\n      {{/if}}\n    {{/if}}\n  </label>\n</div>"
}